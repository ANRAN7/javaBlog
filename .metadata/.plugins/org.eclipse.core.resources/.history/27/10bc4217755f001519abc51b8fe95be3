package com.rlovep.junit;

import org.junit.Test;

public class TestJunit1 {
	@Test
      public void sort(){
    	  int arr[]={9,4,5,8};
    	  for(int j=0;j<arr.length;j++)
    		  for(int i=j;i<arr.length;i++){
    			  if(arr[i]>arr[j]){
    				  int temp=arr[i];
    				  arr[i]=arr[j];
    				  arr[j]=temp;
    				  
    			  }
    		  }
      System.out.println(arr);     
      }
}
